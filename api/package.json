{
  "name": "alcarin-api",
  "version": "0.0.1",
  "scripts": {
    "start": "ts-node-dev -r tsconfig-paths/register --no-notify --watch openapi.yml --respawn --transpileOnly -- ./src/index.ts",
    "queue": "ts-node-dev --no-notify --respawn --transpileOnly -- ./src/queue/run.ts",
    "cli": "node -r ts-node/register --experimental-repl-await bin/cli.ts",
    "test": "env $(cat .env.test | xargs) mocha -r tsconfig-paths/register -r ts-node/register --timeout 10000 --paths 'src/index.spec.ts' 'src/**/*.spec.ts'",
    "test:watch": "npm test -- --watch --watch-files 'src/**/*.ts,./openapi.yml'",
    "check-types": "tsc --noEmit",
    "lint": "eslint . --ext js --ext ts",
    "check-all": "npm run check-types && npm run lint && npm test",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js"
  },
  "dependencies": {
    "@hapi/boom": "^9.0.0",
    "@hapi/joi": "^17.1.1",
    "ajv": "^6.12.2",
    "bcrypt": "^4.0.1",
    "bull-arena": "^2.8.1",
    "bullmq": "^1.6.8",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "express-openapi-validator": "^3.15.1",
    "http-status-codes": "^1.4.0",
    "ioredis": "^4.14.1",
    "morgan": "^1.9.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^7.17.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.22",
    "uuid": "^8.2.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/bull-arena": "^2.6.0",
    "@types/chai": "^4.2.7",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/express-list-endpoints": "^4.0.1",
    "@types/hapi__joi": "^17.1.0",
    "@types/ioredis": "^4.14.7",
    "@types/mocha": "^5.2.7",
    "@types/morgan": "^1.7.37",
    "@types/multer": "^1.4.3",
    "@types/node": "^13.13.2",
    "@types/passport": "^1.0.3",
    "@types/passport-jwt": "^3.0.3",
    "@types/sinon": "^7.5.1",
    "@types/sinon-chai": "^3.2.3",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^2.15.0",
    "@typescript-eslint/parser": "^2.15.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.2",
    "fs-extra": "^8.1.0",
    "mocha": "^7.2.0",
    "prettier": "1.19.1",
    "sinon": "^8.1.1",
    "sinon-chai": "^3.4.0",
    "supertest": "^4.0.2",
    "ts-mocha": "^7.0.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  }
}
